#include <stdio.h>
int OperationChoices(int c, int a , int b )
{
    if(c==1){
        return a+b;
    }
    else if(c==2)
    {
        return a-b;
    }
    else if(c==3)
    {
        return a*b;
    }
    else if(c==4)
    {
        return a/b;
    }
}

int main()
{
    int a,b,c;
    scanf("%d",&a);
    scanf("%d",&b);
    scanf("%d",&c);
    int result=OperationChoices(a,b,c );
    printf("%d",result);
    return 0;
}
