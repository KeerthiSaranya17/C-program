#include <stdio.h>
int oddevensequence(int a[],int n){
    for(int i=0;i<n;i++){
        if(a[i]%2==0){
            printf("even");
        }
        else{
            printf("odd");
        }
    }
    printf("\n");
}
int main()
{
    int a[]={1,2,3,4,5,6};
    int n=sizeof(a) / sizeof(a[0]);;
    oddevensequence(a,n);
    return 0;
}
